[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
req_extensions     = v3_req
distinguished_name = dn

[ dn ]
CN = chainsensors-broker

[ v3_req ]
keyUsage            = keyEncipherment, dataEncipherment, digitalSignature
extendedKeyUsage    = serverAuth
subjectAltName      = @alt_names

[ alt_names ]
IP.1  = 192.168.11.105
DNS.1 = localhost
DNS.2 = chainsensors-broker
